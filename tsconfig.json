{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/contracts": [
        "libs/contracts/src"
      ],
      "@app/contracts/*": [
        "libs/contracts/src/*"
      ],
      "@app/database-orderhub": [
        "libs/database-orderhub/src"
      ],
      "@app/database-orderhub/*": [
        "libs/database-orderhub/src/*"
      ],
      "@app/database-scrooge": [
        "libs/database-scrooge/src"
      ],
      "@app/database-scrooge/*": [
        "libs/database-scrooge/src/*"
      ],
      "@app/database-tiktok": [
        "libs/database-tiktok/src"
      ],
      "@app/database-tiktok/*": [
        "libs/database-tiktok/src/*"
      ],
      "@app/logging": [
        "libs/logging/src"
      ],
      "@app/logging/*": [
        "libs/logging/src/*"
      ],
      "@app/health": [
        "libs/health/src"
      ],
      "@app/health/*": [
        "libs/health/src/*"
      ]
    }
  }
}